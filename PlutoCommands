-- Scripts:

local function MYIWKRZ_fake_script() -- CommandsFrame.DragScript 
	local script = Instance.new('LocalScript', CommandsFrame)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local starPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(starPos.X.Scale, starPos.X.Offset + delta.X, starPos.Y.Scale, starPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
			dragToggle = true
			dragStart = input.Position
			starPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(MYIWKRZ_fake_script)()
local function NROEE_fake_script() -- CommandsFrame.CommandSearchScript 
	local script = Instance.new('LocalScript', CommandsFrame)

	local search = script.Parent.CommandsSearch
	local items = script.Parent.Items
	
	function UpdateResults()
		local search = string.lower(search.Text)
		for i, v in pairs(items:GetChildren()) do
			if v:IsA("TextLabel") then
				if search ~= "" then
					local item = string.lower(v.Text)
					if string.find(item, search) then
						v.Visible = true
					else
						v.Visible = false
					end
				else
						v.Visible = true
				end
			end
		end
	end
	
	search.Changed:Connect(UpdateResults)
end
coroutine.wrap(NROEE_fake_script)()
local function JLNZLI_fake_script() -- Commands.WorkingClose 
	local script = Instance.new('LocalScript', Commands)

	local poi = script.Parent.CommandsFrame
	local Ex = poi.ExitButton
	
	Ex.MouseButton1Down:connect(function() 
		poi.Visible = false
	end)
end
coroutine.wrap(JLNZLI_fake_script)()
