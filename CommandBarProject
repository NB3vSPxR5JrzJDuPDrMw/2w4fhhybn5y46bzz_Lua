-- ALL TWEENS BELOW, IGNORE THESE PERMANTLY
-- Tweens
local function ULYZ_fake_script() -- PoisonBar.tweenbar 
	local script = Instance.new('LocalScript', PoisonBar)


	local Bar = script.Parent.CMDBar
	local Open = false
	local plr = game.Players.LocalPlayer

	local PositionClosed = UDim2.new(-0.3, 0,0.888, 0)
	local PositionOpen = UDim2.new(0.011, 0,0.888, 0)



	plr:GetMouse().KeyDown:Connect(function(K)
		if K == ';' then
			Input:CaptureFocus()
			Input.Text = ""
			Bar:TweenPosition((PositionOpen),'Out','Linear',.25)
		end
	end)
	Input.FocusLost:Connect(function(enterPressed)
		if enterPressed then 
			Bar:TweenPosition((PositionClosed),"Out","Linear",.25)
		end
	end)
end
coroutine.wrap(ULYZ_fake_script)()
-- End Of Tweens
