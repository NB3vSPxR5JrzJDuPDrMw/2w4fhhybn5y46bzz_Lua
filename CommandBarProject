
local PoisonBar = Instance.new("ScreenGui")
local CMDBar = Instance.new("Frame")
local Input = Instance.new("TextBox")
local Gradient = Instance.new("Frame")
local UIGradient = Instance.new("UIGradient")

PoisonBar.Name = "PoisonBar"
PoisonBar.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
PoisonBar.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
PoisonBar.ResetOnSpawn = false
CMDBar.Name = "CMDBar"
CMDBar.Parent = PoisonBar
CMDBar.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
CMDBar.BackgroundTransparency = 0.100
CMDBar.BorderSizePixel = 0
CMDBar.Position = UDim2.new(-0.300000012, 0, 0.888000011, 0)
CMDBar.Size = UDim2.new(0, 225, 0, 29)
Input.Name = "Input"
Input.Parent = CMDBar
Input.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Input.BackgroundTransparency = 1.000
Input.Size = UDim2.new(0, 224, 0, 29)
Input.Font = Enum.Font.Gotham
Input.PlaceholderText = "Command"
Input.Text = ""
Input.TextColor3 = Color3.fromRGB(255, 255, 255)
Input.TextSize = 15.000
Gradient.Name = "Gradient"
Gradient.Parent = CMDBar
Gradient.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Gradient.BorderColor3 = Color3.fromRGB(27, 42, 53)
Gradient.BorderSizePixel = 0
Gradient.Position = UDim2.new(0, 0, 1, 0)
Gradient.Size = UDim2.new(0, 225, 0, 3)
UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(57, 104, 252)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(51, 68, 175))}
UIGradient.Parent = Gradient


-- Doomspire BrickBattle Commands 

--1D
Input.FocusLost:Connect(function()
local arg = string.split(Input.Text)
  if arg[1] == ";yellow" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(130.996, 45.2, 1.5e-05)
end
end)

--2D
Input.FocusLost:Connect(function()
local arg = string.split(Input.Text)
  if arg[1] == ";blue" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-0.344067,24.8,131.682)
end
end)

--3D
Input.FocusLost:Connect(function()
local arg = string.split(Input.Text)
  if arg[1] == ";red" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-130.75, 24.8,-0.00576636)
end
end)

--4D
Input.FocusLost:Connect(function()
local arg = string.split(Input.Text)
  if arg[1] == ";green" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0.356766, 24.8, -130.937)
end
end)

--5D
Input.FocusLost:Connect(function()
local arg = string.split(Input.Text)
  if arg[1] == ";re" then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, -499, 0)
end
end)
--END OF DOOMSPIRE BRICKBATTLE SHIT















